# IBM Rally MCP Server Environment Variables
# Copy this file to .env and fill in your actual values

# Rally API Configuration
RALLY_APIKEY=your_rally_api_key_here
RALLY_INSTANCE=your_rally_instance_url_here
RALLY_PROJECT_NAME=your_default_project_name_here

# Logging Configuration
LOG_LEVEL=info

# Optional: HTML stripping for test case descriptions
STRIP_HTML_TESTCASE_DESCRIPTION=true
